package JavaConcepts;

public class JavaForLoopreverseArray {

	public static void main(String[] args) {
		int array[]= new int[5];
		array[0]=10;
		array[1]=20;
		array[2]=30;
		array[3]=40;
		array[4]=50;
		
		int count=array.length;
		
		for(int i=count; i<count;i=count-1)
		{
			System.out.println(array[i]);
		}

	}

}
