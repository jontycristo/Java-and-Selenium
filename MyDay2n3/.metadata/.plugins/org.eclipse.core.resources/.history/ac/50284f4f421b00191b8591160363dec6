
public class JavaPoly {
	
	class Super {
	    public int index = 1;
	}

	class App extends Super {

	    public App(int index) {
	        index = index;
	    }

	}


	public static void main(String[] args) {
		// TODO Auto-generated method stub
		App myApp = new App(10);
        System.out.println(myApp.index);

	}

}
